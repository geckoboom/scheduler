{
    "name": "geckoboom/scheduler",
    "description": "Cron scheduler for console commands",
    "keywords": [
        "scheduler"
    ],
    "type": "library",
    "license": "BSD-3-Clause",
    "autoload": {
        "psr-4": {
            "Geckoboom\\Scheduler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Geckoboom\\Scheduler\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Artem Tsyganov",
            "email": "ontroip@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4",
        "psr/simple-cache": ">=1.0",
        "dragonmantank/cron-expression": "^3.1",
        "symfony/process": ">3.3.0",
        "psr/container": "^1.0||^2.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "4.0.x-dev",
        "dg/bypass-finals": "dev-master",
        "phpunit/phpunit": "9.5.x-dev"
    }
}
