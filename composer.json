{
    "name": "geckoboom/whirlwind-scheduler",
    "description": "Cron scheduler for console commands",
    "keywords": [
        "whirlwind",
        "scheduler"
    ],
    "type": "library",
    "license": "BSD-3-Clause",
    "autoload": {
        "files": ["src/helpers.php"],
        "psr-4": {
            "Geckoboom\\WhirlwindScheduler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Geckoboom\\WhirlwindScheduler\\Test\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Artem Tsyganov",
            "email": "ontroip@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "~7.4",
        "whirlwind-framework/framework": "dev-main",
        "dragonmantank/cron-expression": "dev-master",
        "psr/simple-cache": "^1.0",
        "symfony/process": "5.4.x-dev",
        "league/container": "4.x-dev"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "4.0.x-dev",
        "dg/bypass-finals": "dev-master",
        "phpunit/phpunit": "9.5.x-dev"
    }
}
